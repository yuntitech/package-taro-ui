#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/leejunhui/Develop/React/Library/taro-ui/node_modules/.pnpm/rollup-plugin-visualizer@4.0.2_rollup@2.79.1/node_modules/rollup-plugin-visualizer/bin/node_modules:/Users/leejunhui/Develop/React/Library/taro-ui/node_modules/.pnpm/rollup-plugin-visualizer@4.0.2_rollup@2.79.1/node_modules/rollup-plugin-visualizer/node_modules:/Users/leejunhui/Develop/React/Library/taro-ui/node_modules/.pnpm/rollup-plugin-visualizer@4.0.2_rollup@2.79.1/node_modules:/Users/leejunhui/Develop/React/Library/taro-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/leejunhui/Develop/React/Library/taro-ui/node_modules/.pnpm/rollup-plugin-visualizer@4.0.2_rollup@2.79.1/node_modules/rollup-plugin-visualizer/bin/node_modules:/Users/leejunhui/Develop/React/Library/taro-ui/node_modules/.pnpm/rollup-plugin-visualizer@4.0.2_rollup@2.79.1/node_modules/rollup-plugin-visualizer/node_modules:/Users/leejunhui/Develop/React/Library/taro-ui/node_modules/.pnpm/rollup-plugin-visualizer@4.0.2_rollup@2.79.1/node_modules:/Users/leejunhui/Develop/React/Library/taro-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../rollup-plugin-visualizer/bin/cli.js" "$@"
else
  exec node  "$basedir/../rollup-plugin-visualizer/bin/cli.js" "$@"
fi
